{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "TaskFlow (HTTPS)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/TaskFlow.Web/bin/Debug/net9.0/TaskFlow.Web.dll",
      "args": [],
      "cwd": "${workspaceFolder}/TaskFlow.Web",
      "stopAtEntry": false,
      "launchBrowser": {
        "enabled": true,
        "args": "https://localhost:7136"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "TaskFlow (HTTP)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/TaskFlow.Web/bin/Debug/net9.0/TaskFlow.Web.dll",
      "args": [],
      "cwd": "${workspaceFolder}/TaskFlow.Web",
      "stopAtEntry": false,
      "launchBrowser": {
        "enabled": true,
        "args": "http://localhost:5092"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
